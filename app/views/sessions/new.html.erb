<%= render '/shared/content_title', title: 'Login Page' %>

<div class='well'>
  <%= form_tag '/login' do %>
    <div class='form-group'>
      <%= label_tag :username %>
      <%= text_field_tag :username %>
    </div>
    <div class='form-group'>
      <%= label_tag :password %>
      <%= password_field_tag :password %>
    </div>
    <div class='form-group'>
      <%= submit_tag 'Login' %>
    </div>
  <% end %>
</div>