<%= render '/shared/content_title', title: 'Search for players near you!' %>

<%= form_tag '/search' do %>
  <%= label_tag :radius %>
  <%= text_field_tag :radius %>
  <%= submit_tag 'Search' %>
<% end %>

<ul>
  <% @users.each do |user| %>
    <li><%= link_to user.username, user_path(user) %></li>
  <% end %>
</ul>
  