= render '/shared/content_title', title: 'Edit your profile'

= bootstrap_form_for @user do |f|
  = f.select :role, [['Top', 'top'], ['Mid', 'mid'], ['Jungle', 'jungle'], ['ADC', 'adc'], ['Support', 'support']]
  = f.select :tier, [['Bronze', 'bronze'], ['Silver', 'silver'], ['Gold', 'gold'], ['Platinum', 'platinum'], ['Diamond', 'diamond'], ['Master', 'master'], ['Challenger', 'challenger']]
  = f.password_field :password
  = f.primary 'Update'